    "configuration": {
      "title": "Explorer for Endevor",
      "properties": {
        "endevor.locations": {
          "deprecationMessage": "Please, use the extension UI to modify the extension locations instead.",
          "type": "array",
          "title": "Endevor locations",
          "description": "List of Endevor locations to load on extension startup",
          "scope": "resource",
          "items": {
            "type": "object",
            "properties": {
              "service": {
                "type": "string",
                "description": "Endevor profile name"
              },
              "elementLocations": {
                "type": "array",
                "description": "Endevor location profile names",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "service",
              "elementLocations"
            ]
          }
        },
        "endevor.maxParallelRequests": {
          "type": "number",
          "title": "Endevor HTTP WebServices parallel requests amount",
          "description": "Number of parallel HTTP requests that are supported by Endevor",
          "scope": "resource",
          "default": 4
        },
        "endevor.automaticSignOut": {
          "type": "boolean",
          "scope": "resource",
          "title": "Automatic sign-out",
          "description": "Automatically sign elements out",
          "default": false
        },
        "endevor.authWithToken": {
          "type": "boolean",
          "scope": "resource",
          "title": "Use token authentication",
          "description": "By default always try to authenticate to Endevor Web Services using the temporary tokens (PassTickets). When it is not configured on the EWS side or disabled, basic authentication with username/passphrase is used instead.",
          "default": true
        },
        "endevor.fileExtensionResolution": {
          "title": "File extension resolution",
          "description": "Specifies the method of the file extension resolution for the locally saved elements",
          "type": "string",
          "enum": [
            "Element name only",
            "Endevor type file extension only",
            "Endevor type file extension or type name"
          ],
          "enumDescriptions": [
            "Uses the element name to determine the file extension",
            "Uses the Endevor defined file extension for the type",
            "Uses the Endevor defined file extension for the type or type name as a fall-back option"
          ],
          "default": "Endevor type file extension or type name",
          "scope": "resource"
        },
        "endevor.generateAfterEdit": {
          "title": "Generate after edit",
          "description": "Specifies if any generate action should be taken after the element is edited",
          "type": "string",
          "enum": [
            "Generate the element",
            "Do not generate the element",
            "Ask if the element should be generated"
          ],
          "enumDescriptions": [
            "Generates the element after edit",
            "Does not generate the element after edit",
            "Asks if the element should be generated after edit"
          ],
          "default": "Ask if the element should be generated",
          "scope": "resource"
        },
        "profiles.keepInSync": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "title": "Keep in sync with Zowe CLI",
          "description": "Keep Endevor configuration in sync with Zowe profiles or a Team Configuration file"
        },
        "endevor.experimental.workspaceSync": {
          "type": "boolean",
          "scope": "resource",
          "title": "Endevor workspace sync",
          "description": "Enable Endevor workspace synchronization capabilities for the opened workspace",
          "default": false
        }
      }
    },
    "walkthroughs": [
      {
        "id": "e4e.gettingStartedWalkthrough",
        "title": "Get Started with Explorer for Endevor",
        "description": "Work on your Endevor project using VS Code.",
        "steps": [
          {
            "id": "e4e.gettingStarted.openExtension",
            "title": "Open the Explorer for Endevor Tree View",
            "description": "The extension offers a custom Tree View to work with Endevor elements.\n[Open Extension](command:e4e.treeView.focus)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/resources/logo.png",
              "altText": "Link to a remote image with an extension logo."
            },
            "completionEvents": [
              "onCommand:e4e.treeView.focus"
            ]
          },
          {
            "id": "e4e.gettingStarted.defineConnectionDetails",
            "title": "Connect to the Endevor Web Services",
            "description": "Create a new Endevor connection or import existing connection profile from Zowe.\n[Connect to Endevor](command:e4e.addNewService)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/E4E-add-service.gif",
              "altText": "Link to a remote gif file with an example from the extension UI."
            },
            "completionEvents": [
              "onCommand:e4e.addNewService"
            ]
          },
          {
            "id": "e4e.gettingStarted.defineSearchLocation",
            "title": "Search for elements",
            "description": "Create a new Endevor inventory location or import an existing location profile from Zowe.\n[Search for Elements](command:e4e.addNewSearchLocation)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/E4E-add-search-location.gif",
              "altText": "Link to a remote gif file with a search example from the extension UI."
            },
            "completionEvents": [
              "onCommand:e4e.addNewSearchLocation"
            ]
          },
          {
            "id": "e4e.gettingStarted.openMoreWalkthroughs",
            "title": "Learn how to use the extension",
            "description": "Learn how to get started with Explorer for Endevor, using a series of practical guides.\n[Select a Guide](command:welcome.showAllWalkthroughs)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/E4E-more-guides.gif",
              "altText": "Link to a remote gif file with a walkthrough search example."
            },
            "completionEvents": [
              "onCommand:welcome.showAllWalkthroughs"
            ]
          }
        ]
      },
      {
        "id": "e4e.exploreSyncWorkspaceWalkthrough",
        "title": "Introducing the Endevor Synchronized Workspace",
        "description": "Work with the whole Endevor inventory locations locally using the synchronized workspaces.",
        "steps": [
          {
            "id": "e4e.exploreSync.enableSync",
            "title": "Enable Endevor Workspace synchronization feature",
            "description": "To work with Endevor synchronized workspaces, please enable workspace synchronization in your settings.\n[Enable](command:workbench.action.openSettings?%5B%22endevor.workspaceSync%22%5D)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Setting.gif",
              "altText": "Link to a remote gif file with a setting change example from the VSCode UI."
            },
            "completionEvents": [
              "command:workbench.action.openSettings",
              "onSettingChanged:config.endevor.experimental.workspaceSync",
              "onContext:config.endevor.experimental.workspaceSync"
            ]
          },
          {
            "id": "e4e.exploreSync.openLocalFolder",
            "title": "Select a Local Folder",
            "description": "Select a folder to store and work with Endevor inventory location elements locally.\n[Select Folder](command:vscode.openFolder)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Folder.gif",
              "altText": "Link to a remote gif file with selecting a folder example from the VSCode UI."
            },
            "completionEvents": [
              "onCommand:vscode.openFolder",
              "onContext:e4e.scm.status == 'initialized'"
            ]
          },
          {
            "id": "e4e.exploreSync.initializeWorkspace",
            "title": "Initialize an Endevor Workspace",
            "description": "Mark folder as an Endevor workspace to keep your own copy of the remote Endevor inventory locally.\n[Initialize an Endevor Workspace](command:e4e.sync.initWorkspace)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Init.gif",
              "altText": "Link to a remote gif file with a workspace init example from the extension UI."
            },
            "completionEvents": [
              "onCommand:e4e.sync.initWorkspace",
              "onContext:e4e.scm.status == 'initialized'"
            ]
          },
          {
            "id": "e4e.exploreSync.pullFromEndevor",
            "title": "Pull Endevor Elements",
            "description": "Retrieve all elements from the remote inventory location to the initialized Endevor workspace. If you need more elements, add them to your remote inventory location and pull again.\n[Pull From Endevor](command:e4e.sync.pullFromEndevor)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Pull.gif",
              "altText": "Link to a remote gif file with a pull example from the extension UI."
            },
            "completionEvents": [
              "onCommand:e4e.sync.pullFromEndevor"
            ]
          },
          {
            "id": "e4e.exploreSync.editElements",
            "title": "Work with an entire Endevor inventory location",
            "description": "All the retrieved elements are regular files now. You can create new, modify, or remove them locally.\n[Show local workspace](command:workbench.files.action.focusFilesExplorer)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Workspace.gif",
              "altText": "Link to a remote gif file with an element change example from the VSCode UI."
            },
            "completionEvents": [
              "onCommand:workbench.files.action.focusFilesExplorer"
            ]
          },
          {
            "id": "e4e.exploreSync.syncWithEndevor",
            "title": "Synchronize Endevor Workspace",
            "description": "Synchronize the whole local workspace with local changes back to apply them to the remote inventory location. Resolve conflicts and synchronize back again if needed.\n[Synchronize Changes](command:e4e.sync.syncWorkspace)",
            "media": {
              "image": "https://raw.githubusercontent.com/eclipse/che-che4z-explorer-for-endevor/master/packages/explorer-for-endevor/images/Endevor_Sync_Sync.gif",
              "altText": "Link to a remote gif file with a sync example from the extension UI."
            },
            "completionEvents": [
              "onCommand:e4e.sync.syncWorkspace"
            ]
          }
        ]
      }
    ],
    "viewsWelcome": [
      {
