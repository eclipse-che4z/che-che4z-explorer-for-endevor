      {
        "view": "scm",
        "contents": "To learn more about the Endevor Workspace synchronization feature, read [this article](https://medium.com/modern-mainframe/editing-synchronized-endevor-elements-locally-ff096d09eb5e) on Medium or review the [Endevor Synchronized workspace Walkthough](command:workbench.action.openWalkthrough?%5B%22BroadcomMFD.explorer-for-endevor%23e4e.exploreSyncWorkspaceWalkthrough%22%2C%20false%5D) in VSCode.",
        "when": "config.endevor.experimental.workspaceSync"
      }
    ]
  },
  "scripts": {
    "build:prod": "rimraf dist && webpack --config webpack.prod.js",
    "build:dev": "rimraf dist && webpack --config webpack.dev.js",
    "build:watch": "rimraf dist && webpack --config webpack.dev.js --watch",
    "type": "tsc --watch --noEmit",
    "lint": "concurrently -n \"typescpt,_cycles_,_eslint_,prettier\" \"tsc --noemit \" \"madge --circular --extensions ts ./\" \"eslint --ext .ts .\" \"prettier --check .\"",
    "eslint:watch": "eslint --ext .ts . --watch",
    "detect-cycles": "madge --circular --extensions ts ./",
    "pretty": "prettier --write .",
    "test": "yarn run test:no-vscode && yarn run test:vscode",
    "test:no-vscode": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watch",
    "test:vscode": "ts-node ./vscode-test.run.ts",
    "tsc:watch": "tsc --noemit --watch",
    "package": "mkdirp vsce && vsce package --yarn -o vsce/explorer-for-endevor.vsix",
    "publish:vsce": "vsce publish --pat",
    "publish:ovsx": "ovsx publish --pat",
    "verify-pat:vsce": "vsce verify-pat --pat",
    "verify-pat:ovsx": "ovsx verify-pat --pat",
    "clean": "rimraf dist results vsce node_modules .vscode-test LICENSE.txt NOTICE.md",
    "clean:test": "rimraf .vscode-test",
    "copy-into-repo-root": "copyfiles README.md CHANGELOG.md LICENSE.txt NOTICE.md CONTRIBUTING.md ../../"
  },
  "dependencies": {
    "@broadcom/endevor-for-zowe-cli": "^7.6.0",
    "@local/endevor": "1.0.0",
    "@local/extension": "1.0.0",
    "@local/profiles": "1.0.0",
    "@local/telemetry": "1.0.0",
    "@local/type-parser": "1.0.0",
    "@local/vscode-wrapper": "1.0.0",
    "@local/views": "1.0.0",
    "table": "^6.8.1"
  },
  "extensionDependencies": [
    "Zowe.vscode-extension-for-zowe"
  ]
}
