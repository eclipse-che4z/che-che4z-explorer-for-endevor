{
  "name": "explorer-for-endevor",
  "displayName": "Explorer for Endevor",
  "description": "Extension that modernizes the way you interact with Endevor inventory locations and elements",
  "author": "Broadcom",
  "version": "1.7.0-BETA",
  "publisher": "BroadcomMFD",
  "homepage": "https://github.com/eclipse/che-che4z-explorer-for-endevor",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/che-che4z-explorer-for-endevor"
  },
  "bugs": {
    "url": "https://github.com/eclipse/che-che4z-explorer-for-endevor/issues"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "icon": "resources/logo.png",
  "engines": {
    "node": "^18.16.0",
    "vscode": "^1.76.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "keywords": [
    "zOS",
    "mainframe",
    "explorer",
    "endevor",
    "zowe"
  ],
  "activationEvents": [
    "workspaceContains:.endevor"
  ],
  "main": "./dist/extension.bundle.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "e4eExplorerContainer",
          "title": "Explorer for Endevor",
          "icon": "./resources/logo.svg"
        }
      ],
      "panel": [
        {
          "id": "e4eExplorerPanel",
          "title": "Explorer for Endevor",
          "icon": "./resources/logo.svg"
        }
      ]
    },
    "views": {
      "e4eExplorerContainer": [
        {
          "id": "e4e.treeView",
          "name": "Explorer for Endevor"
        },
        {
          "id": "e4e.elementHistoryView",
          "name": "Element History"
        }
      ],
      "e4eExplorerPanel": [
        {
          "id": "e4e.activityView",
          "name": "Activity"
        }
      ]
    },
    "commands": [
      {
        "command": "e4e.refreshTreeView",
        "title": "Refresh Endevor Tree",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        },
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.refreshHistoryTreeView",
        "title": "Refresh/Retrieve Element History",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        },
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.submitIssue",
        "title": "Submit an Extension Issue",
        "icon": "$(feedback)",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.addServiceAndLocation",
        "title": "Add an Endevor Connection and Inventory Location",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.addNewService",
        "title": "Add a New Endevor Connection",
        "icon": {
          "light": "resources/light/add.svg",
          "dark": "resources/dark/add.svg"
        },
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.addNewSearchLocation",
        "title": "Add a New Inventory Location",
        "icon": {
          "light": "resources/light/add.svg",
          "dark": "resources/dark/add.svg"
        },
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.addElementFromFileSystem",
        "title": "Add an Element",
        "icon": "$(file-add)"
      },
      {
        "command": "e4e.showFirstFoundElements",
        "title": "Show Endevor Elements Up the Map",
        "icon": "$(arrow-up)"
      },
      {
        "command": "e4e.showElementsInPlace",
        "title": "Show Endevor Elements in Place",
        "icon": "$(eye-closed)"
      },
      {
        "command": "e4e.showEmptyTypes",
        "title": "Show Empty Types",
        "icon": "$(empty-window)"
      },
      {
        "command": "e4e.hideEmptyTypes",
        "title": "Hide Empty Types",
        "icon": "$(clear-all)"
      },
      {
        "command": "e4e.hideService",
        "title": "Hide an Endevor Connection",
        "icon": "$(eye-closed)"
      },
      {
        "command": "e4e.editService",
        "title": "Edit an Endevor Connection",
        "icon": "$(edit)"
      },
      {
        "command": "e4e.editConnectionDetails",
        "title": "Repair Endevor Connection Details",
        "icon": "$(wrench)"
      },
      {
        "command": "e4e.testConnectionDetails",
        "title": "Test Endevor Connection Details",
        "icon": "$(refresh)"
      },
      {
        "command": "e4e.editCredentials",
        "title": "Repair Endevor Credentials",
        "icon": "$(wrench)"
      },
      {
        "command": "e4e.clearSearchLocationFilters",
        "title": "Clear All Filters",
        "icon": "$(trash)"
      },
      {
        "command": "e4e.clearSearchLocationFilter",
        "title": "Clear Filter",
        "icon": "$(trash)"
      },
      {
        "command": "e4e.clearSearchLocationFilterValue",
        "title": "Clear Filter Value",
        "icon": "$(trash)"
      },
      {
        "command": "e4e.editSearchLocationFilter",
        "title": "Edit Filter",
        "icon": "$(edit)"
      },
      {
        "command": "e4e.filterSearchLocationByElementName",
        "title": "By Element Name..."
      },
      {
        "command": "e4e.filterSearchLocationByElementType",
        "title": "By Element Type..."
      },
      {
        "command": "e4e.filterSearchLocationByElementCcid",
        "title": "By Element Last Action CCID..."
      },
      {
        "command": "e4e.hideSearchLocation",
        "title": "Hide an Inventory Location",
        "icon": "$(eye-closed)"
      },
      {
        "command": "e4e.deleteSearchLocation",
        "title": "Delete an Inventory Location",
        "icon": "$(trash)"
      },
      {
        "command": "e4e.deleteService",
        "title": "Delete an Endevor Connection",
        "icon": "$(trash)"
      },
      {
        "command": "e4e.viewElementDetails",
        "title": "View Details"
      },
      {
        "command": "e4e.retrieveElement",
        "title": "Retrieve"
      },
      {
        "command": "e4e.retrieveElementWithDependencies",
        "title": "Retrieve with Dependencies"
      },
      {
        "command": "e4e.createPackage",
        "title": "Create Package"
      },
      {
        "command": "e4e.editElement",
        "title": "Edit"
      },
      {
        "command": "e4e.moveElement",
        "title": "Move"
      },
      {
        "command": "e4e.generateElement",
        "title": "Generate in Place",
        "icon": "$(check)"
      },
      {
        "command": "e4e.generateElementWithCopyBack",
        "title": "Generate with Copyback",
        "icon": "$(arrow-down)"
      },
      {
        "command": "e4e.generateElementWithNoSource",
        "title": "Generate with No Source"
      },
      {
        "command": "e4e.printListing",
        "title": "Print a Listing"
      },
      {
        "command": "e4e.printHistory",
        "title": "Show History"
      },
      {
        "command": "e4e.diff.discardChanges",
        "title": "Discard Changes",
        "icon": "$(discard)"
      },
      {
        "command": "e4e.diff.acceptChanges",
        "title": "Accept Changes",
        "icon": "$(check)"
      },
      {
        "command": "e4e.signOutElement",
        "title": "Sign Out"
      },
      {
        "command": "e4e.signInElement",
        "title": "Sign In"
      },
      {
        "command": "e4e.cleanupStorage",
        "title": "Clear Configuration Storage",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.dumpStorageData",
        "title": "Dump Configuration Storage Data",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.migrateLocationsFromSettings",
        "title": "Migrate Connections/Inventory Locations",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.sync.initWorkspace",
        "title": "Initialize a Workspace",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.sync.syncWorkspace",
        "title": "Synchronize a Workspace",
        "icon": "$(repo-sync)",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.sync.pullFromEndevor",
        "title": "Pull from Endevor",
        "icon": "$(arrow-down)",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.showGettingStartedWalkthrough",
        "title": "Show the Getting Started Walkthrough",
        "category": "Explorer for Endevor"
      },
      {
        "command": "e4e.sync.openElement",
        "title": "Open Element",
        "category": "Explorer for Endevor",
        "icon": "$(go-to-file)"
      },
      {
        "command": "e4e.sync.discardChanges",
        "title": "Discard Element Changes",
        "category": "Explorer for Endevor",
        "icon": "$(discard)"
      },
      {
        "command": "e4e.sync.discardAllChanges",
        "title": "Discard All Element Changes",
        "category": "Explorer for Endevor",
        "icon": "$(discard)"
      },
      {
        "command": "e4e.sync.revertSectionChange",
        "title": "Revert Element Change",
        "category": "Explorer for Endevor",
        "icon": "$(discard)"
      },
      {
        "command": "e4e.sync.confirmConflictResolution",
        "title": "Confirm Element Conflict Resolution",
        "category": "Explorer for Endevor",
        "icon": "$(add)"
      },
      {
        "command": "e4e.sync.confirmAllConflictResolutions",
        "title": "Confirm All Element Conflict Resolutions",
        "category": "Explorer for Endevor",
        "icon": "$(add)"
      },
      {
        "command": "e4e.generateSubsystemElements",
        "title": "Generate All Elements",
        "category": "Explorer for Endevor",
        "icon": "$(checklist)"
      },
      {
        "command": "e4e.viewTypeDetails",
        "title": "View Type Details"
      }
    ],
    "submenus": [
      {
        "id": "e4e.generateElementUpTheMap",
        "label": "Generate element"
      },
      {
        "id": "e4e.filterSearchLocationMenu",
        "label": "Filter an Inventory Location",
        "icon": "$(filter)"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "e4e.sync.syncWorkspace",
          "when": "scmProvider == e4e.scm",
          "group": "navigation"
        },
        {
          "command": "e4e.sync.pullFromEndevor",
          "when": "scmProvider == e4e.scm",
          "group": "navigation"
        },
        {
          "command": "e4e.sync.pullFromEndevor",
          "group": "1_header@1",
          "when": "scmProvider == e4e.scm"
        }
      ],
      "scm/resourceGroup/context": [
        {
          "command": "e4e.sync.discardAllChanges",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.changes",
          "group": "inline"
        },
        {
          "command": "e4e.sync.confirmAllConflictResolutions",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.mergeChanges",
          "group": "inline"
        }
      ],
      "scm/resourceState/context": [
        {
          "command": "e4e.sync.openElement",
          "when": "scmProvider == e4e.scm && scmResourceState != ELEMENT/REMOVED",
          "group": "1_open"
        },
        {
          "command": "e4e.sync.openElement",
          "when": "scmProvider == e4e.scm && scmResourceState != ELEMENT/REMOVED",
          "group": "inline@1"
        },
        {
          "command": "e4e.sync.discardChanges",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.changes",
          "group": "2_changes"
        },
        {
          "command": "e4e.sync.discardChanges",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.changes",
          "group": "inline@2"
        },
        {
          "command": "e4e.sync.confirmConflictResolution",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.mergeChanges",
          "group": "2_conflicts"
        },
        {
          "command": "e4e.sync.confirmConflictResolution",
          "when": "scmProvider == e4e.scm && scmResourceGroup == e4e.scm.group.mergeChanges",
          "group": "inline@2"
        }
      ],
      "scm/change/title": [
        {
          "command": "e4e.sync.revertSectionChange",
          "when": "config.endevor.experimental.workspaceSync && e4e.scm.status == 'initialized'"
        }
      ],
      "e4e.generateElementUpTheMap": [
        {
          "command": "e4e.generateElementWithCopyBack",
          "group": "e4e_generate_element_actions@0"
        },
        {
          "command": "e4e.generateElementWithNoSource",
          "group": "e4e_generate_element_actions@1"
        },
        {
          "command": "e4e.generateElement",
          "group": "e4e_generate_element_actions@2"
        }
      ],
      "e4e.filterSearchLocationMenu": [
        {
          "command": "e4e.filterSearchLocationByElementName",
          "group": "1_e4e_filter_location_actions@0"
        },
        {
          "command": "e4e.filterSearchLocationByElementType",
          "group": "1_e4e_filter_location_actions@1"
        },
        {
          "command": "e4e.filterSearchLocationByElementCcid",
          "group": "1_e4e_filter_location_actions@2"
        },
        {
          "command": "e4e.clearSearchLocationFilters",
          "group": "2_e4e_filter_location_clear"
        }
      ],
      "view/title": [
        {
          "command": "e4e.refreshTreeView",
          "when": "view == e4e.treeView",
          "group": "navigation"
        },
        {
          "command": "e4e.addNewService",
          "when": "view == e4e.treeView",
          "group": "navigation"
        },
        {
          "command": "e4e.submitIssue",
          "when": "view == e4e.treeView",
          "group": "navigation"
        },
        {
          "command": "e4e.refreshHistoryTreeView",
          "when": "view == e4e.elementHistoryView && e4e.elementHistoryView.showRefresh",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "e4e.diff.discardChanges",
          "group": "navigation",
          "when": "isInDiffEditor && resourceDirname in e4e.editedFolders && isWeb != true"
        },
        {
          "command": "e4e.diff.acceptChanges",
          "group": "navigation",
          "when": "isInDiffEditor && resourceDirname in e4e.editedFolders && isWeb != true"
        }
      ],
      "view/item/context": [
        {
          "command": "e4e.viewElementDetails",
          "when": "viewItem == ELEMENT_IN_PLACE || viewItem == ELEMENT_UP_THE_MAP",
          "group": "1_e4e_view_element_actions"
        },
        {
          "command": "e4e.editElement",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "2_e4e_edit_element_actions@0"
        },
        {
          "submenu": "e4e.generateElementUpTheMap",
          "group": "2_e4e_generate_element_actions@0",
          "when": "!listMultiSelection && viewItem == ELEMENT_UP_THE_MAP"
        },
        {
          "command": "e4e.generateElement",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE",
          "group": "2_e4e_generate_element_actions@0"
        },
        {
          "command": "e4e.printListing",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "2_e4e_generate_element_actions@1"
        },
        {
          "command": "e4e.printHistory",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "2_e4e_generate_element_actions@1"
        },
        {
          "command": "e4e.moveElement",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "3_e4e_move_element_actions@0"
        },
        {
          "command": "e4e.retrieveElement",
          "when": "viewItem == ELEMENT_IN_PLACE || viewItem == ELEMENT_UP_THE_MAP",
          "group": "4_e4e_retrieve_actions@0"
        },
        {
          "command": "e4e.retrieveElementWithDependencies",
          "when": "viewItem == ELEMENT_IN_PLACE || viewItem == ELEMENT_UP_THE_MAP",
          "group": "4_e4e_retrieve_actions@1"
        },
        {
          "command": "e4e.signOutElement",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "5_e4e_sign_actions@0"
        },
        {
          "command": "e4e.signInElement",
          "when": "!listMultiSelection && viewItem == ELEMENT_IN_PLACE || !listMultiSelection && viewItem == ELEMENT_UP_THE_MAP",
          "group": "5_e4e_sign_actions@1"
        },
        {
          "command": "e4e.createPackage",
          "when": "viewItem == ELEMENT_IN_PLACE || viewItem == ELEMENT_UP_THE_MAP",
          "group": "5_e4e_package_actions@0"
        },
        {
          "command": "e4e.hideSearchLocation",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && viewItem =~ /\\/(INVALID_CREDENTIALS|INVALID_CONNECTION)/",
          "group": "inline"
        },
        {
          "command": "e4e.hideSearchLocation",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && !(viewItem =~ /\\/NON_EXISTING/)",
          "group": "e4e_search_location_actions@0"
        },
        {
          "command": "e4e.deleteSearchLocation",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && viewItem =~ /\\/NON_EXISTING/",
          "group": "inline"
        },
        {
          "command": "e4e.deleteSearchLocation",
          "when": "viewItem == LOCATION || viewItem =~ /^LOCATION\\// || viewItem == LOCATION_PROFILE/NON_EXISTING",
          "group": "e4e_search_location_actions@1"
        },
        {
          "submenu": "e4e.filterSearchLocationMenu",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && viewItem =~ /\\/VALID$/",
          "group": "inline@0"
        },
        {
          "command": "e4e.addElementFromFileSystem",
          "when": "(viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && viewItem =~ /\\/VALID$/) || viewItem == TYPE",
          "group": "inline@3"
        },
        {
          "command": "e4e.showFirstFoundElements",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && !(viewItem =~ /\\/WITH_MAP/) && viewItem =~ /\\/VALID$/",
          "group": "inline@1"
        },
        {
          "command": "e4e.showElementsInPlace",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && viewItem =~ /\\/WITH_MAP/ && viewItem =~ /\\/VALID$/",
          "group": "inline@1"
        },
        {
          "command": "e4e.showEmptyTypes",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && !(viewItem =~ /\\/WITH_EMPTY_TYPES/) && viewItem =~ /\\/VALID$/",
          "group": "inline@2"
        },
        {
          "command": "e4e.hideEmptyTypes",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)/ && viewItem =~ /\\/WITH_EMPTY_TYPES/ && viewItem =~ /\\/VALID$/",
          "group": "inline@2"
        },
        {
          "command": "e4e.clearSearchLocationFilters",
          "when": "viewItem == FILTERED",
          "group": "inline"
        },
        {
          "command": "e4e.clearSearchLocationFilters",
          "when": "viewItem == FILTERED",
          "group": "e4e_filter_actions"
        },
        {
          "command": "e4e.editSearchLocationFilter",
          "when": "viewItem == FILTER",
          "group": "inline@0"
        },
        {
          "command": "e4e.clearSearchLocationFilter",
          "when": "viewItem == FILTER",
          "group": "inline@1"
        },
        {
          "command": "e4e.editSearchLocationFilter",
          "when": "viewItem == FILTER",
          "group": "e4e_filter_actions@0"
        },
        {
          "command": "e4e.clearSearchLocationFilter",
          "when": "viewItem == FILTER",
          "group": "e4e_filter_actions@1"
        },
        {
          "command": "e4e.clearSearchLocationFilterValue",
          "when": "viewItem == FILTER_VALUE",
          "group": "inline"
        },
        {
          "command": "e4e.clearSearchLocationFilterValue",
          "when": "viewItem == FILTER_VALUE",
          "group": "e4e_filter_actions"
        },
        {
          "command": "e4e.editConnectionDetails",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && viewItem =~ /\\/INVALID_CONNECTION/",
          "group": "inline"
        },
        {
          "command": "e4e.testConnectionDetails",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && viewItem =~ /\\/INVALID_CONNECTION/",
          "group": "inline"
        },
        {
          "command": "e4e.editCredentials",
          "when": "viewItem =~ /^(LOCATION|LOCATION_PROFILE)\\// && viewItem =~ /\\/INVALID_CREDENTIALS/",
          "group": "inline"
        },
        {
          "command": "e4e.hideService",
          "when": "viewItem == SERVICE/INVALID_CREDENTIALS || viewItem == SERVICE_PROFILE/INVALID_CREDENTIALS || viewItem == SERVICE/INVALID_CONNECTION || viewItem == SERVICE_PROFILE/INVALID_CONNECTION",
          "group": "inline"
        },
        {
          "command": "e4e.editService",
          "when": "viewItem == SERVICE || viewItem == SERVICE/INVALID_CREDENTIALS || viewItem == SERVICE/INVALID_CONNECTION",
          "group": "e4e_service_actions@0"
        },
        {
          "command": "e4e.hideService",
          "when": "viewItem == SERVICE || viewItem == SERVICE_PROFILE || viewItem == SERVICE/INVALID_CREDENTIALS || viewItem == SERVICE_PROFILE/INVALID_CREDENTIALS || viewItem == SERVICE/INVALID_CONNECTION || viewItem == SERVICE_PROFILE/INVALID_CONNECTION",
          "group": "e4e_service_actions@1"
        },
        {
          "command": "e4e.deleteService",
          "when": "viewItem == SERVICE/NON_EXISTING || viewItem == SERVICE_PROFILE/NON_EXISTING",
          "group": "inline"
        },
        {
          "command": "e4e.deleteService",
          "when": "viewItem == SERVICE || viewItem == SERVICE/NON_EXISTING || viewItem == SERVICE_PROFILE/NON_EXISTING || viewItem == SERVICE/INVALID_CREDENTIALS || viewItem == SERVICE/INVALID_CONNECTION",
          "group": "e4e_service_actions@2"
        },
        {
          "command": "e4e.addNewSearchLocation",
          "when": "viewItem == SERVICE || viewItem == SERVICE_PROFILE",
          "group": "inline"
        },
        {
          "command": "e4e.generateSubsystemElements",
          "when": "viewItem == SUB",
          "group": "2_e4e_generate_element_actions@0"
        },
        {
          "command": "e4e.generateSubsystemElements",
          "when": "viewItem == SUB",
          "group": "inline"
        },
        {
          "command": "e4e.viewTypeDetails",
          "when": "viewItem == TYPE"
        }
      ],
      "commandPalette": [
        {
          "command": "e4e.addElementFromFileSystem",
          "when": "false"
        },
